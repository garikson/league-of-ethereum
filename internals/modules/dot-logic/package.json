{
  "name": "dot-logic",
  "version": "1.0.0",
  "description": "A super tiny side-effects handler based on redux-logic (without debouncing/limiting)",
  "main": "index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "build": "babel src --out-dir ./lib/",
    "test": "mocha test.index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/silentcicero/dot-logic.git"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs"
    ]
  },
  "keywords": [
    "dot",
    "logic",
    "redux"
  ],
  "author": "Nick Dodson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silentcicero/dot-logic/issues"
  },
  "homepage": "https://github.com/silentcicero/dot-logic#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "redux": "^3.6.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "html-webpack-plugin": "2.28.0",
    "json-loader": "0.5.4",
    "lint-staged": "3.3.0",
    "mocha": "3.2.0",
    "mocha-babel": "3.0.3",
    "offline-plugin": "4.6.1",
    "plop": "1.7.4",
    "pre-commit": "1.2.2",
    "regenerator-runtime": "0.10.1",
    "wafr": "^0.3.5",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.1"
  }
}
